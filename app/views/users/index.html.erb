  <div class="container pt-5">
    <div class="row">
      <div class="col-md-8 offset-md-2">
        <h2>会員一覧</h2>
       <table class="table table-hover table-inverse">
          
            <tr>
              <th class= "text-center">プロフィール画像</th>
              <th class= "text-center">名前</th>
              <th class= "text-center">性別</th>
              <th class= "text-center">年齢</th>
              <th colspan "3"></th>
              <th colspan "3"></th>
            </tr>
          
        
            <% @users.each do |user| %>
            <tr>
              <td class= "text-center">
                <%= attachment_image_tag user, :profile_image, size: "60x60", format: "jpeg",fallback: "no_image.jpg" %>
              </td>
              <td class= "text-center">
                <%= link_to user.name, user_path(user.id),class:'nav-link' %>
              </td>
              <td class= "text-center"><%=user.sex %></td>
              <td class= "text-center"><%=user.age %></td>
              <td class= "text-center">フォロー数: <%= user.followings.count %></td>
              <td class= "text-center">フォロワー数: <%= user.followers.count %></td>
            </tr>
            <% end %>
        </table>
      </div>
    </div>
  </div>